<p id="notice"><%= notice %></p>

<h1>Inscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Local trabalho</th>
      <th>Municipio</th>
      <th>Uf</th>
      <th>Regime juridico</th>
      <th>Cargo funcao</th>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Contato</th>
      <th>Responsavel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inscriptions.each do |inscription| %>
      <tr>
        <td><%= inscription.nome %></td>
        <td><%= inscription.endereco %></td>
        <td><%= inscription.bairro %></td>
        <td><%= inscription.local_trabalho %></td>
        <td><%= inscription.municipio %></td>
        <td><%= inscription.uf %></td>
        <td><%= inscription.regime_juridico %></td>
        <td><%= inscription.cargo_funcao %></td>
        <td><%= inscription.rg %></td>
        <td><%= inscription.cpf %></td>
        <td><%= inscription.email %></td>
        <td><%= inscription.contato %></td>
        <td><%= inscription.responsavel %></td>
        <td><%= link_to 'Show', inscription %></td>
        <td><%= link_to 'Edit', edit_inscription_path(inscription) %></td>
        <td><%= link_to 'Destroy', inscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inscription', new_inscription_path %>
